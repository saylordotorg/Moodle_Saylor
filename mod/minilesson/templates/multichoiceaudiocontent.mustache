
  {{#sentences}}
      <div class="row mcplayrow">
          <div class="col-md-1 mcaudioplayer_{{indexplusone}}">
              <div class="mc_playbutton_container">
              <button class="minilesson_mc_audioplayer mcplaytrigger btn btn-secondary" data-index="{{indexplusone}}" id="{{uniqueid}}_audioplayer{{indexplusone}}"><i class="fa fa-play minilesson_mc_playstate"></i></button>
              </div>
          </div>
          <div class="col-md-11 mcaudiotranscript_{{index}}">
              <div class="minilesson_mc_response" data-index="{{indexplusone}}" id="{{uniqueid}}_option{{indexplusone}}" name="{{uniqueid}}_options" >
                  <span class="minilesson_sentence">{{displaysentence}}</span>
                  <span class="minilesson_mc_resultbox">
                  <i class="fa fa-square-o fa-lg minilesson_mc_unanswered"></i>
                  <i class="fa fa-check fa-lg minilesson_mc_right" style="display: none;"></i>
                  <i class="fa fa-times fa-lg minilesson_mc_wrong" style="display: none;"></i>
                  </span>
              </div>
          </div>
      </div>
  {{/sentences}}

